{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "server": "json-server --watch server/db.json",
    "build": "vite build",
    "lint": "npm run lint-eslint && npm run lint-css",
    "lint-eslint": "eslint --ext .js,.jsx src/ *.js",
    "lint-css": "stylelint \"src/**/*.{js,jsx}\"",
    "preview": "vite preview",
    "prepare": "husky install",
    "cypress": "cypress open",
    "cy:run-e2e": "cypress run --e2e --browser chrome && nyc report --reporter=text-summary --report-dir=./coverage/e2e",
    "cy:run-component": "cypress run --component --browser chrome && nyc report --reporter=text-summary --report-dir=./coverage/component"
  },
  "dependencies": {
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@reduxjs/toolkit": "^1.9.5",
    "cypress-fail-on-console-error": "^5.0.0",
    "cypress-real-events": "^1.10.3",
    "date-fns": "^2.30.0",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-day-picker": "^8.8.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.3.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.0.7",
    "swiper": "^10.3.1",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.4",
    "@vitejs/plugin-react": "^4.0.0",
    "cypress": "^12.17.4",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "json-server": "^0.17.3",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^3.0.0",
    "start-server-and-test": "^2.0.1",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "vite": "^4.3.9",
    "vite-plugin-istanbul": "^5.0.0"
  }
}